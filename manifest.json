{
    "manifest_version": 2,
    "name": "Nice",
    "description": "This extension acknoledges all 69's",
    "version": "0.0.1",
    "icons": {
        "16": "daisy_16.png",
        "48": "daisy_48.png",
        "128": "daisy_128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "all_frames": true,
            "js": [
                "nice.js"
            ],
            "run_at": "document_end"
        }
    ]
}